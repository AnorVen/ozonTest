wrapper() {
  margin 0 auto;
  max-width: 960px;
  @media screen and (max-width: 767px){
    max-width: 767px;
    margin-left 15px;
    margin-right:15px;
  }
};

wrapper-small() {
  margin: 0 auto;
  max-width: 745px;

};
clearfix(){
  &:after {
    content: "";
    display: block;
    clear: both;
    visibility: hidden;
    height: 0;
  }
}
visually-hidden() {
  position: absolute;
  width: 1px;
  height: 1px;
  margin: -1px;
  padding: 0;
  border: 0;
  clip: rect(0 0 0 0);
  overflow: hidden;
}
