.wrapper {
  wrapper()
}

.anima {
  position: relative
  &__tea {
    position: absolute;
    top: 120px;
    left: 126px;
    animation: tea 2s infinite steps(1, end);
    @media screen and (max-width: 840px){
      width: 170px;
      height:auto;
    }
    @media mobile-width{
      animation: none;
      visually-hidden()
    }
  }
  &__bosh {
    position: absolute;
    top: 116px;
    right: -2px;
    transform: scale(0.87);
    animation: bosh 2s infinite steps(1, end);
    @media mobile-width{
      animation: none;
      visually-hidden()
    }
  }
  &__tooth {
    position: absolute;
    top: 96px;
    left: 70px;
    animation: tooth 2s infinite steps(1, end);
    @media mobile-width{
      animation: none;
      visually-hidden()
    }
  }
  &__toy {
    position: absolute;
    top: 114px;
    right__toy: 125px;
    transform: scale(0.9);
    animation: toy 2s infinite steps(1, end);
    @media mobile-width{
      animation: none;
      visually-hidden()
    }
  }
}
  @keyframes tea{
    from{
      transform: scale(1);
      top: 120px;
      left: 126px;
    }
    25%{
      transform: scale(1);
      top: 120px;
      left: 126px;
    }
    50%{
      transform: scale(1.2);
      top: 111px;
      left: 141px;
    }
    75%{
      transform: scale(1.2);
      top: 120px;
      left: 140px;
    }
    to{
      top: 120px;
      left: 126px;
      transform scale(1)
    }

  }

  @keyframes bosh{
    from{
      right: 0;
      left: auto
    }
    25%{
      right: auto;
      left: 0;
    }
    50%{
      right: auto;
      left: 0;
    }
    75%{
      left: auto
      right: -5px;
    }
    to{

      right: 0;
      left: auto
    }

  }
  @keyframes tooth{
    from{
      top: 96px;
      left: 70px;
    }
    22%{
      left: auto;
      right: 53px;
    }
    50%{
      left: auto;
      right: 53px;
    }
    75%{
      top: 96px;
      left: 75px;
    }

    to{
      top: 96px;
      left: 70px;
    }

  }
  @keyframes toy{
    from{
      transform: scale(0.9);
      top: 114px;
      right: 125px;
    }
    25%{
      transform: scale(0.9);
      top: 114px;
      right: 125px;
    }
    50%{
      transform: rotate(11deg) scale(0.9);
      top: 115px;
      right: 113px;
    }
    75%{
      transform: rotate(-16deg) scale(0.9);
      top: 118px;
      right: 124px;
    }
    to{
      transform: scale(0.9);
      top: 114px;
      right: 125px;
    }

  }

