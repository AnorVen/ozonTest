.body {
  margin: 0;
  line-height: normal;
  font-family: "Arial", sans-serif;
  width: 100%;
  color: #020202
}

.visually-hidden {
  position: absolute;
  width: 1px;
  height: 1px;
  margin: -1px;
  padding: 0;
  border: 0;
  clip: rect(0 0 0 0);
  overflow: hidden;
}
img{
  max-width: 100%;
  height: auto;
}
